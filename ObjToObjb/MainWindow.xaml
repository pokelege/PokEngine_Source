<Window x:Class="ObjToObjb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ObjToObjb" Height="100" Width="270" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="50"/>
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="0" Name="Filename"/>
        <Button Grid.Column="1" Content="Browse" Click="Import" Width="50"/>
        <Button Grid.ColumnSpan="2" Name="StartB" Grid.Row="1" Content="Start" Click="Start"/>
    </Grid>
</Window>
